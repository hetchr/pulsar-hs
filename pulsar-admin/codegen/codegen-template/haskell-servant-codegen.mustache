name:                {{package}}
version:             0.1.0.0
synopsis:            Auto-generated API bindings for {{package}}
description:         Please see README.md
homepage:            https://openapi-generator.tech
author:              Author Name Here
maintainer:          author.name@email.com
copyright:           YEAR - AUTHOR
category:            Web
build-type:          Simple
cabal-version:       >=1.10

library
  hs-source-dirs:      lib
  exposed-modules:     {{title}}.API
                     , {{title}}.Types
  ghc-options:         -Wall
  build-depends:       base
                     , aeson >= 1.4 && < 2
                     , containers >= 0.6 && < 1
                     , http-types >= 0.12 && < 1
                     , network-uri >= 2.6 && < 3
                     , servant-client >= 0.16 && < 0.21
                     , servant-client-core >= 0.16 && < 0.21
                     , servant >= 0.16 && < 0.21
                     , time >= 1.9 && < 2
                     , text >= 1.2 && < 2
  default-language:    Haskell2010
